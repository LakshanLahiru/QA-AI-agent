{
  "type": "module",
  "name": "mobile-ai-webdriver-tests",
  "version": "0.1.0",
  "description": "WebdriverIO + TypeScript mobile test framework (POM) for AI agent driven test generation.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "wdio run wdio.conf.ts",
    "test:browserstack:ios": "wdio run wdio.browserstack.ios.conf.ts",
    "test:ios": "wdio run wdio.ios.conf.ts",
    "allure:generate": "allure generate ./allure-results --clean -o ./allure-report",
    "allure:open": "allure open ./allure-report",
    "crawl:login": "cross-env CRAWL_PAGE_NAME=login wdio run wdio.conf.ts --spec ./src/tests/crawl-page.e2e.ts"
  },
  "keywords": [
    "webdriverio",
    "mobile",
    "typescript",
    "page-object-model",
    "ai-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@wdio/allure-reporter": "^9.0.0",
    "@wdio/local-runner": "^9.0.0",
    "@wdio/mocha-framework": "^9.0.0",
    "@wdio/spec-reporter": "^9.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "webdriverio": "^9.0.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.3",
    "@types/node": "^20.0.0",
    "@wdio/appium-service": "^9.21.0",
    "@wdio/browserstack-service": "^9.21.0",
    "@wdio/cli": "^9.0.0",
    "allure-commandline": "^2.34.1",
    "chai": "^6.2.1",
    "cross-env": "^7.0.3",
    "typescript": "^5.7.0"
  }
}
